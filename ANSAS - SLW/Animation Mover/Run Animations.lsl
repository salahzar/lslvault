default
{
    state_entry()
    {
        llSetTimerEvent(1);
    }
    timer()
    {
        llSetText("new frame index: "+(string)index,<1,1,1>,1);
        string s=llList2String(animations,index);
        while(s!="END" && index<llGetListLength(animations))
        {
            list bits=llCSV2List(s);
            integer channel=llList2Integer(bits,1); bits=llListReplaceList(bits,[],0,0);
            //llSay(0,s);
            llRegionSay(channel,llList2CSV(bits));
            index++;
            s=llList2String(animations,index);
        } index++;
        if(index>=llGetListLength(animations)) index=0;
        
    }
}